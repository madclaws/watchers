{
  "compilerOptions": {
      "target": "ES2019",
      "module": "CommonJS",
      "sourceMap": true,
      "noImplicitAny": false,
      "strict": false,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
  },
  "typedocOptions": {
    "mode": "modules",
    "out": "docs"
  }
}